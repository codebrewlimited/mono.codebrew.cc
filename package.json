    "postinstall": "npx husky init",
    "lint": "npm run lint --workspaces",
    "test": "npm test --workspaces"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codebrewlimited/mono.codebrew.cc.git"
  },
  "keywords": [],
